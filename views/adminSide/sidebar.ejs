<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>E-Commerce Admin Dashboard</title>
    <link rel="stylesheet" href="/css/admin.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
</head>
<body>
    <div class="wrapper">
        <!-- Sidebar -->
        <nav id="sidebar">
            <div class="sidebar-header">
                <img src="/images/logo.png" alt="logo">
            </div>
            <div id="button-div" class="list-unstyled components">
                <button class="active" id="dashboard-link"><i class="fas fa-tachometer-alt"></i> Dashboard</button>
                <button id="products-link"><i class="fas fa-box"></i> Products</button>
                <button id="orders-link"><i class="fas fa-shopping-cart"></i> Orders</button>
                <button id="customers-link"><i class="fas fa-users"></i> Customers</button>
                <button id="categories-link"><i class="fas fa-list-alt"></i> Categories</button>
                <button id="reports-link"><i class="fas fa-chart-line"></i> Reports</button>
                <button id="settings-link"><i class="fas fa-cog"></i> Settings</button>
            </div>
        </nav>
